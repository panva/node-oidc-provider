{
  "extends": "airbnb-base",
  "rules": {
    "strict": 0,
    "no-empty": [2, { "allowEmptyCatch": true }],
    "no-underscore-dangle": [1, { "allowAfterThis": true }],
    "generator-star-spacing": [1, "both"],
    "no-cond-assign": [2, "except-parens"],
    "no-param-reassign": [2, { "props": false }]
  }
}
